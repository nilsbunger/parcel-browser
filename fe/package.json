{
  "name": "my-project",
  "description": "Frontend for Parsnip by Home3",
  "private": true,
  "version": "0.0.1",
  "source": "src/index.html",
  "//": [
    "type:module means treat everything as an ES module (import / export syntax) without requiring .mjs files",
    "exports in package.json is not supported in parcel yet (until 2.8.4): https://github.com/parcel-bundler/parcel/issues/4155",
    "alias is added temporarily until parcel supports exports in package.json",
    ""
  ],
  "alias": {
    "@refinedev/inferencer/mantine": "./node_modules/@refinedev/inferencer/dist/mantine.js"
  },
  "type": "module",
  "scripts": {
    "dev": "parcel watch --target default",
    "build": "rm -rf .parcel-cache/ dist/ ../dist/ && parcel build --dist-dir ../dist/static --no-cache --target default && mkdir -p ../dist/django-templates && mv ../dist/static/*.html ../dist/django-templates/",
    "lint": "eslint . --ext .ts,.tsx",
    "prettify": "prettier --write .",
    "test": "jest",
    "clean": "rm -rf .parcel-cache/ dist/ ../dist/"
  },
  "targets": {
    "default": {
      "publicUrl": "/static/",
      "source": [
        "partials/headend.partial.html",
        "partials/app.partial.html",
        "partials/login.partial.html"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },

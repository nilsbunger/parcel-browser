{
  "extends": "@parcel/config-default",
  "transformers": {
    "jsx:*.svg": ["...", "@parcel/transformer-svg-react"],
    "jsx:*": ["..."]
  },
  "compressors": {
    "*.{html,css,js,svg,map}": [
      "...",
      "@parcel/compressor-gzip"
    ]
  },

  "reporters":  [
    "...",
    "parcel-reporter-static-files-copy",
    "@parcel/reporter-bundle-analyzer"
  ]
}
